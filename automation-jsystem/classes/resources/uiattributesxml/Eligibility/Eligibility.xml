<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<UIAttributes 
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
          xsi:schemaLocation="UIAttributes.xsd">
	<UIAttribute>
		<locator>ProvID</locator>
		<style>DropDown</style>
		<displayName>Agency</displayName>
		<description>Provide agency name</description>
	</UIAttribute>
	
	<UIAttribute>
		<locator>hic</locator>
		<style>Text</style>
		<displayName>HIC</displayName>
		<description>Provide Health Insurance Claim name</description>
	</UIAttribute>
	
	<UIAttribute>
		<locator>lname</locator>
		<style>Text</style>
		<displayName>Last Name</displayName>
		<description>Last name of the patient</description>
	</UIAttribute>
	
	<UIAttribute>
		<locator>fname</locator>
		<style>Text</style>
		<displayName>First Name</displayName>
		<description>First name of the patient</description>
	</UIAttribute>
	
	<UIAttribute>
		<locator>dob</locator>
		<style>DatePicker</style>
		<displayName>DOB</displayName>
		<description>Pick date of birth of the patient</description>
	</UIAttribute>
	
	<UIAttribute>
		<locator>sex</locator>
		<style>DropDown</style>
		<displayName>Sex</displayName>
		<description>Select the gender of the patient</description>
	</UIAttribute>
	
	<UIAttribute>
		<locator>app_date</locator>
		<style>DatePicker</style>
		<displayName>Application Date</displayName>
		<description>Pick the applcation date</description>
	</UIAttribute>
	
</UIAttributes>