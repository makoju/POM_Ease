<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<UIAttributes 
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
          xsi:schemaLocation="UIAttributes.xsd">
	<!-- My Account Tab - Personal Information Screen UI Elements Start-->
	<UIAttribute>
		<locator>name</locator>
		<style>Text</style>
		<displayName>Name</displayName>
		<description>Customer Name</description>
	</UIAttribute>
	
	<UIAttribute>
		<locator>phone</locator>
		<style>Text</style>
		<displayName>Phone</displayName>
		<description>Customer Phone Number</description>
	</UIAttribute>
	
	<UIAttribute>
		<locator>email</locator>
		<style>Text</style>
		<displayName>Email</displayName>
		<description>Customer Email</description>
	</UIAttribute>
	

	<!-- My Account Tab - Personal Information Screen UI Elements End-->
	<!--My Account Tab - Setup Alert Options Start-->
	
	<UIAttribute>
		<locator>Setup Alerts</locator>
		<style>Link</style>
		<displayName>Setup Alerts</displayName>
		<description>Setup Alerts For a User</description>
	</UIAttribute>
	
	<UIAttribute>
		<locator>//*[@id='form1']/table</locator>
		<style>SetupAlert</style>
		<displayName>Setup Alerts Options</displayName>
		<description>Provide alert options along with ON or OFF by separating with comma, e.g., Job Failure:ON,Failed HIC Upload Report:OFF</description>
	</UIAttribute>
	<!--My Account Tab - Setup Alert Options End-->
	
	<UIAttribute>
		<locator>submit</locator>
		<style>submit</style>
		<displayName>Submit</displayName>
		<description>Submit</description>
	</UIAttribute>
	
</UIAttributes>